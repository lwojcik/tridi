{
  "name": "tridi",
  "version": "2.0.3",
  "description": "JavaScript 360 3D Product Viewer",
  "main": "dist/js/tridi.min.js",
  "types": "dist/js/tridi.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "clean": "rimraf dist/js/*",
    "serve": "serve dist/",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "build": "npm run clean && webpack",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tridijs/tridi.git"
  },
  "keywords": [
    "3d",
    "360",
    "image",
    "photo",
    "product",
    "view",
    "viewer"
